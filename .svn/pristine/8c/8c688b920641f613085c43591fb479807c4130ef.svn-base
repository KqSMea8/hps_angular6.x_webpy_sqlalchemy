@import "styles.less";
.background-mask{
    position: absolute;
    top:0;
    left:0;
    z-index: 998;

    width: 100%;
    height: 100%;
    background: #000;
    opacity: .2;
}

.loading-box {
    .loading {
        z-index: 999;
        background: #fff;
        opacity: 1;
        border-radius: @DefaultborderRadius;

        width: 100px;
        height: 100px;
    }
}