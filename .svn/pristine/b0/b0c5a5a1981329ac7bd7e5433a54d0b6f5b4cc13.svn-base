@import "styles.less";

.content{
    height: ~"calc(100% - 70px)";

    >div{
        height: 100%;
        background-color: #eaf1f9;

        .main{
            padding: @defaultPadding;
            margin: @defaultPadding;
            background-color: #fff;

            >div{
                h1{
                    font-size: 18px;
                    font-weight: bolder;
                    margin-bottom: 20px;
                }

                .item-content{
                    border: 1px solid #ccc;
                    padding: @defaultPadding;
                }
            }
        }
    }
}