@import "styles.less";
.log {
    overflow: hidden;
    background: #eaf1f9;

    padding: @defaultPadding;
    
    ul {
        padding-left: 0;
        margin-bottom: 0;
    }

    .search-condition {
        overflow: hidden;
        background: #fff;
        margin-bottom: 10px;
        padding: @defaultPadding;

        .item {
            padding-bottom: 15px;

            &:not(:first-child) {
                padding-top: 15px;
                border-top: 1px dashed #ccc;
            }

            .condition-list {
                display: flex;
                align-items: center;
                
                li {
                    margin: 5px 20px 5px 0;
                    
                    p {
                        display: flex;
                        align-items: center;
                        margin-bottom: 10px;
                    }
    
                    span {
                        margin: 0 @defaultPadding;
                    }
                    nz-select {
                        margin-right: 10px;
                    }
                }
            }
        }

    }
    
    .search-data-list {
        overflow: hidden;
        background: #fff;
    }
}